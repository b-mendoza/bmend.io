{
  "name": "intro-to-nextjs__remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "vite build && vite build --ssr",
    "dev": "vite dev",
    "fix": "pnpm run --sequential \"/fix:.*/\"",
    "fix:eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "fix:prettier": "prettier --ignore-unknown --cache --write .",
    "lint": "pnpm run --sequential \"/lint:.*/\"",
    "lint:eslint": "eslint --cache-location ./node_modules/.cache/eslint .",
    "lint:prettier": "prettier --ignore-unknown --cache --check .",
    "lint:types": "tsc --pretty",
    "start": "remix-serve ./build/index.js"
  },
  "dependencies": {
    "@heroicons/react": "2.0.18",
    "@remix-run/node": "2.3.1",
    "@remix-run/react": "2.3.1",
    "@remix-run/serve": "2.3.1",
    "@unpic/react": "0.1.3",
    "isbot": "3.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "2.3.1",
    "@remix-run/v1-route-convention": "0.1.4",
    "@tailwindcss/forms": "0.5.7",
    "@types/node": "20.10.0",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-xo": "0.43.1",
    "eslint-config-xo-react": "0.27.0",
    "eslint-config-xo-typescript": "1.0.1",
    "eslint-plugin-deprecation": "2.0.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "postcss": "8.4.31",
    "prettier": "3.1.0",
    "prettier-plugin-packagejson": "2.4.6",
    "prettier-plugin-tailwindcss": "0.5.7",
    "tailwindcss": "3.3.5",
    "typescript": "5.3.2",
    "vite": "5.0.2",
    "vite-tsconfig-paths": "4.2.1"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}
